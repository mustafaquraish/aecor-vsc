{
    "name": "aecor",
    "displayName": "Aecor",
    "description": "Syntax Highlighting for Aecor",
    "repository": "https://github.com/mustafaquraish/aecor-vsc",
    "publisher": "MustafaQuraish",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "aecor",
                "aliases": [
                    "aecor"
                ],
                "extensions": [
                    ".ae"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "aecor",
                "scopeName": "source.aecor",
                "path": "./syntaxes/aecor.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "aecor",
                "path": "./snippets/aecor.json"
            }
        ]
    }
}
